generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Filme {
  id        Int      @id @default(autoincrement())
  titulo    String
  sinopse   String
  genero    String
  diretor   String
  ano       Int
  duracao   Int
  idioma    String
  createdAt DateTime @default(now())
}
